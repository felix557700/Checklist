{
  "name": "checklist",
  "version": "0.0.1",
  "repository": "https://github.com/felix557700/checklist.git",
  "description": "mainly travel checklist",
  "keywords": "nodejs expressjs aurelia.js babel gulp checklist",
  "main": "./app/server.js",
  "engines": {
    "node": ">=5.0.0"
  },
  "scripts": {
    "start": "node ./app/server.js",
    "babel": "babel backend -d app",
    "babel-watch": "babel backend -d app --watch",
    "bundle": "webpack --colors --display-modules --display-error-details --config webpack.config.js",
    "bundle-watch": "webpack --watch --colors --display-modules --display-error-details --config webpack.config.js",
    "bundle-production": "webpack -p --config webpack.prod.config.js",
    "build-heroku": "./node_modules/.bin/gulp dev",
    "build": "npm run babel && ./node_modules/.bin/gulp build:frontend",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "angular": "^1.5.2",
    "angular-ui-router": "^0.2.18",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-node5": "^1.1.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.5.2",
    "compression": "^1.6.2",
    "del": "^2.2.1",
    "express": "^4.14.0",
    "express-jwt": "^3.4.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^2.0.11",
    "gulp-concat": "^2.6.0",
    "gulp-nodemon": "^2.1.0",
    "gulp-relative-sourcemaps-source": "^0.1.2",
    "gulp-sass": "^2.3.2",
    "gulp-shell": "^0.5.2",
    "gulp-sourcemaps": "^1.6.0",
    "jsonwebtoken": "^5.7.0",
    "merge-stream": "^1.0.0",
    "mongodb": "^2.2.0",
    "morgan": "^1.2.2",
    "node-uuid": "^1.4.7",
    "nodemon": "^1.9.2",
    "run-sequence": "^1.2.2",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "node-inspector": "^0.12.6"
  },
  "author": "Filip Vitas",
  "license": "ISC"
}
